            </svg>
        </div>
        <!-- /loader-->
    </div>
    <!-- loader backdrop -->
    <!-- Page container -->
    <div class="gx-container">
        <!-- Page Sidebar -->
        <div id="menu" class="side-nav gx-sidebar">
            <div class="navbar-expand-lg">
                <!-- Sidebar header  -->
                <div class="sidebar-header">
                    <div class="user-profile">
                        <img class="img-responsive logo_side_bar" alt="Kotak_Logo" src="~/Audit_assets/images/logo_side_bar.png">
                    </div>
                </div>
                <!-- /sidebar header -->
                <!-- Main navigation -->
                <div id="main-menu" class="main-menu navbar-collapse collapse">
                    <ul class="nav-menu">
                        <li class="menu no-arrow">
                            <a href="/Home/Dashboard/">
                                <span class="nav-text">Dashboard Success Report</span>
                            </a>
                        </li>
                        <li class="menu no-arrow">
                            <a href="/Home/Dashboard_Failure/">
                                <span class="nav-text">Dashboard Failure Report</span>
                            </a>
                        </li>
                        <li class="menu no-arrow">
                            <a href="/Home/Index/">
                                <span class="nav-text">Audit Log</span>
                            </a>
                        </li>
                        <li class="menu no-arrow">
                            <a href="/Home/Clearcrn/">
                                <span class="nav-text">Clear CRN</span>
                            </a>
                        </li>
                        <li class="menu no-arrow">
                            <a href="/Home/DataMigration/">
                                <span class="nav-text">Data Migration</span>
                            </a>
                        </li>
                        <li class="menu no-arrow selected">
                            <a href="/Home/UserFeedback/">
                                <span class="nav-text">Users Feedback</span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- /main navigation -->
            </div>
        </div>
        <!-- /page sidebar -->
        <!-- Main Container -->
        <div class="gx-main-container">
            <!-- Main Header -->
            <header class="main-header">
                <div class="gx-toolbar">
                    <div class="sidebar-mobile-menu d-block d-lg-none">
                        <a class="gx-menu-icon menu-toggle" href="#menu">
                            <span class="menu-icon"></span>
                        </a>
                    </div>
                    <div class="search-bar right-side-icon bg-transparent d-none d-sm-block">
                    </div>
                    <ul class="quick-menu header-notifications ml-auto">
                        <li class="dropdown">
                            <a href="javascript:void(0)" data-toggle="dropdown" aria-haspopup="true" class="d-inline-block" aria-expanded="true">
                                <div class="user-thumb">
                                    <img alt="Domnic Brown" src="~/Audit_assets/images/domnic-brown.jpg"
                                         class="rounded-circle size-40 user-avatar">
                                </div>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right" data-placement="bottom-end" data-x-out-of-boundaries="">
                                <div class="dropdown-menu-perfectscrollbar1">
                                    <div class="messages-list">
                                        <ul class="list-unstyled">
                                            <li class="media">
                                                <div class="user-thumb">
                                                    <i class="fa fa-lock fa_color"></i>
                                                </div>
                                                <div class="media-body">
                                                    <div class="d-flex justify-content-between align-items-center">
                                                        <h4 class="text-capitalize user-name mb-0">
                                                            <button id="logout_btn">Logout</button>
                                                        </h4>
                                                    </div>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </header>
            <!-- /main header -->
            <!-- Main Content -->
            <div class="kotak-main-content">
                <div class="gx-wrapper">
                    <div class="animated slideInUpTiny animation-duration-3">
                        <!--Registration Breadcrumbs start-->
                        <div class="page-heading d-sm-flex justify-content-sm-between align-items-sm-center mb-3">
                            <h2 class="title mb-3 mb-sm-0">User Feedback</h2>
                            <nav class="mb-0 breadcrumb">
                                <a href="/Home/Dashboard" class="breadcrumb-item">Home</a>
                                <a href="/Home/UserFeedback" class="breadcrumb-item">User Feedback</a>
                            </nav>
                        </div>
                        <!--Get the OS and Browser details-->
                        <input type="hidden" value="" id="os_details" />
                        <input type="hidden" value="" id="browser_details" />
                        <!--Get the PID & count details-->
                        <input type="hidden" value="0" id="PID" />
                        <input type="hidden" value="-1" id="send_count" />
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                    <!--Feedback table starts-->
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                            <div class="gx-card" id="feedback_card padding_size" style="padding: 10px 60px !important;">
                                                <div class="gx-card-body">
                                                    <!--Filter div start-->
                                                    <div class="row mt-3 mb-3">
                                                        <div class="col-6 col-sm-6 col-xs-6 col-md-3 col-lg-3 col-xl-3">
                                                            <h4><b>What user like the most?</b></h4>
                                                            Total Responses 22 <button type="button" class="btn btn-link" id="answer1_view" onClick="custom_answer1_popup()">View All</button>

                                                        </div>
                                                        <div class="col-6 col-sm-6 col-xs-6 col-md-3 col-lg-3 col-xl-3">
                                                            <h4>What user Do not like the most?</h4>
                                                            Total Responses 22 <button type="button" class="btn btn-link" id="answer2_view" onClick="custom_answer2_popup()">View All</button>
                                                            
                                                        </div>

                                                        <div class="col-6 col-sm-6 col-xs-6 col-md-6 col-lg-6 col-xl-6 text-right">
                                                            <button class="btn btn-primary" id="filter_popover_btn" onClick="custom_showPopup()">Filters</button>
                                                        </div>
                                                    </div>
                                                    <!--Filter div end-->
                                                    @*
                                            <div class="table-responsive">
                                                    *@
                                                    <div class="row mt-3 mb-3" id="top_div">
                                                        <div class="col-12 col-sm-12 col-xs-12 col-md-6 col-lg-6 col-xl-6 text-left">
                                                            <p class="mt-3">Showing <span id="top_starting_index">1</span> to <span id="top_ending_index">10</span> of <span id="top_total_count">50</span> entries</p>
                                                        </div>
                                                        <div class="col-12 col-sm-12 col-xs-12 col-md-6 col-lg-6 col-xl-6 text-right">
                                                            <div class="button-group">
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary first_btn" title="First" disabled><i class="fa fa-angle-double-left" aria-hidden="true"></i></button>
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary previous_btn" title="Previous" disabled><i class="fa fa-angle-left" aria-hidden="true"></i></button>
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary next_btn" title="Next"><i class="fa fa-angle-right" aria-hidden="true"></i></button>
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary last_btn" title="Last"><i class="fa fa-angle-double-right" aria-hidden="true"></i></button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row" id="data_div">
                                                        <div class="col-12 col-sm-12 col-xs-12 col-md-12 col-lg-12 col-xl-12">
                                                            <table class="table table table-bordered" id="user_feedback_table">
                                                                <thead>
                                                                    <tr>
                                                                        <th>S.No</th>
                                                                        <th>CRN</th>
                                                                        <th>Ratings</th>
                                                                        <th>What do you like the most?</th>
                                                                        <th>What did you not like the most?</th>
                                                                        <th>Suggestion</th>
                                                                        <th>Date & Time</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody></tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                    <div class="card text-center" id="loading_div">
                                                        <div class="card-body">
                                                            <div class="spinner-border avatar-sm text-info" role="status"></div>
                                                        </div>
                                                    </div>
                                                    <div class="card text-center" id="no_data_div">
                                                        <div class="card-body">
                                                            <p class="text-center pb-0">No Data Available</p>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3" id="bottom_div">
                                                        <div class="col-12 col-sm-12 col-xs-12 col-md-6 col-lg-6 col-xl-6 text-left">
                                                            <p class="mt-3">Showing <span id="bottom_starting_index">1</span> to <span id="bottom_ending_index">10</span> of <span id="bottom_total_count">50</span> entries</p>
                                                        </div>
                                                        <div class="col-12 col-sm-12 col-xs-12 col-md-6 col-lg-6 col-xl-6 text-right">
                                                            <div class="button-group">
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary first_btn" title="First" disabled><i class="fa fa-angle-double-left" aria-hidden="true"></i></button>
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary previous_btn" title="Previous" disabled><i class="fa fa-angle-left" aria-hidden="true"></i></button>
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary next_btn" title="Next"><i class="fa fa-angle-right" aria-hidden="true"></i></button>
                                                                <button type="button" class="pagination_btn btn btn-rounded btn-primary last_btn" title="Last"><i class="fa fa-angle-double-right" aria-hidden="true"></i></button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*
                                            </div>
                                                    *@
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--Feedback table ends-->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/gx-wrapper-->
                @* Answer Popup *@

                <div class="answer1content_div" id="answer1_popup">
                    <div id="filter_label_row_id">
                        <table class="table table-borderless  table-sm">
                            <tbody>
                                <tr>
                                    <td>Simplified Process</td>
                                    <td>23 Responses </td>
                                </tr>
                                <tr>
                                    <td>Easy to follow instructions</td>
                                    <td>23 Responses</td>
                                </tr>
                                <tr>
                                    <td>Quick login</td>
                                    <td>23 Responses</td>
                                </tr>
                                <tr>
                                    <td>Face Incorrectly Identified</td>
                                    <td>23 Responses</td>
                                </tr>
                                <tr>
                                    <td>None</td>
                                    <td>23 Responses</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="answer2content_div" id="answer2_popup">
                    <div id="filter_label_row_id">
                        <table class="table table-borderless  table-sm">
                            <tbody>
                                <tr>
                                    <td>Simplified Process</td>
                                    <td>23 Responses </td>
                                </tr>
                                <tr>
                                    <td>Easy to follow instructions</td>
                                    <td>23 Responses</td>
                                </tr>
                                <tr>
                                    <td>Quick login</td>
                                    <td>23 Responses</td>
                                </tr>
                                <tr>
                                    <td>Face Incorrectly Identified</td>
                                    <td>23 Responses</td>
                                </tr>
                                <tr>
                                    <td>None</td>
                                    <td>23 Responses</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                @*Filter PopUp*@
                <div class="popovercontent_div" id="custom_popup">
                    <div id="filter_label_row_id">
                        <div class="row" id="filter_1_row_id">
                            <div class="form-row ml-1 mr-1">
                                <div class="form-group col-4 col-sm-4 col-xs-4 col-md-4 col-lg-4 col-xl-4">
                                    <label class="font-weight-bold">CRN</label>
                                    <input type="text" class="form-control" name="crn" id="crn" placeholder="CRN" />
                                </div>
                                <div class="form-group col-4 col-sm-4 col-xs-4 col-md-4 col-lg-4 col-xl-4">
                                    <label class="font-weight-bold">Suggestion Keyword</label>
                                    <input type="text" class="form-control" name="suggestion" id="suggestion" placeholder="Suggestion Keyword" />
                                </div>
                                <div class="form-group col-4 col-sm-4 col-xs-4 col-md-4 col-lg-4 col-xl-4">
                                    <label class="font-weight-bold">Like the most?</label>
                                    <select id="question_1" class="js-states form-control question1" err_msg_class="question_1_err_msg">
                                        <option selected value="0">All</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-row ml-1 mr-1">
                                <div class="form-group col-4 col-sm-4 col-xs-4 col-md-4 col-lg-4 col-xl-4">
                                    <label class="font-weight-bold">Do not like the most?</label>
                                    <select id="question_2" class="js-states form-control question2" err_msg_class="question_2_err_msg">
                                        <option selected value="0">All</option>
                                    </select>
                                </div>
                                <div class="form-group col-4 col-sm-4 col-xs-4 col-md-4 col-lg-4 col-xl-4">
                                    <label class="font-weight-bold">From Date<sup id="symbol_color">*</sup></label>
                                    <input type="date" class="form-control" name="from_date" id="from_date" onchange="FDate('from_date')" />
                                </div>
                                <div class="form-group col-4 col-sm-4 col-xs-4 col-md-4 col-lg-4 col-xl-4">
                                    <label class="font-weight-bold">To Date<sup id="symbol_color">*</sup></label>
                                    <input type="date" class="form-control" name="to_date" id="to_date" onchange="TDate('to_date_filter')" />
                                </div>
                            </div>
                            <div class="form-row ml-1 mr-1">
                                <div class="form-group col-12 col-sm-12 col-xs-12 col-md-12 col-lg-12 col-xl-12">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <strong>Rating:</strong> <span id="slider-range-value1"></span>-<span id="slider-range-value2"></span>
                                        </div>
                                    </div>
                                    <div class="row slider-labels">
                                        <div class="col-sm-12 ml-2 rating_range">
                                            <div id="slider-range"></div>
                                            <input type="hidden" id="min-value" value="">
                                            <input type="hidden" id="max-value" value="">
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="filter_bottom_btn">
                        <div class="form-group col-8 col-sm-8 col-xs-8 col-md-8 col-lg-8 col-xl-8 float-right" id="button_top">
                            <div class="btn-toolbar float-right" role="toolbar" aria-label="Toolbar with button groups">
                                <div class="btn-group mr-2" role="group" aria-label="First group">
                                    <button type="button" class="btn btn-primary search_userfeedback_button" id="search_btn" data-loading-text="Loading...">Search</button>
                                </div>
                                <div class="btn-group mr-2" role="group" aria-label="Second group">
                                    <button type="button" class="btn btn-primary reset_userfeedback_button" id="reset_btn">Reset</button>
                                </div>
                                <div class="btn-group mr-2" role="group" aria-label="Third group">
                                    <button type="button" class="btn btn-primary reset_userfeedback_button" id="download_btn">Download</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="alert_popup_div"></div>
                <span id="SecondsUntilExpire"></span>
                <!-- Footer -->
                <footer class="gx-footer page_header">
                    <div class="d-flex flex-row justify-content-between">
                        <p> Copyright Kotak Mahindra Bank Limited. © 2019</p>
                    </div>
                </footer>
                <!-- /footer -->
            </div>
                <!-- /main content -->
            </div>
        <!-- /main container -->
    </div>
    <!-- /page container -->
